for (( i = 1; i '<' 100; i ++ ))
do
 echo "---- Jugando mapa $i ----"
  java -jar tools/PlayGame-1.2.jar maps/map$i.txt 1000 1000 log.txt 2> log/map$i.txt "java -jar MyBot/dist/MyBot.jar $1 $2 $3 $4 $5 $6 $7 $8" "java -jar example_bots/DualBot.jar" | > log/smap$i.txt
cat log/map$i.txt
sleep 3
done

